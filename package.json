    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "test": "test",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@unocss/eslint-plugin": "^0.62.3",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "typescript": "^5.5.3",
    "unocss": "^0.62.3",
    "vite": "^5.4.1",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "pnpm run lint:fix"
    ]
